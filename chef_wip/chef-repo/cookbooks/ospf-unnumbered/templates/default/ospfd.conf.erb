hostname ospfd
password cn321
enable password cn321
log timestamp precision 6
log file /var/log/quagga/ospfd.log
router ospf
 ospf router-id <%= @routerid %>
 network <%= @ospfnetwork %> area <%= @ospfarea %>
<% @interfaces.each do |interface| -%>
interface <%= interface %>
   ip ospf network point-to-point
<% end -%>
